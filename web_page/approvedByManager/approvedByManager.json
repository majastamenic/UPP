        "\t\t\taddress: $data.jobApplication.address !== undefined ? $data.jobApplication.address : null,",
        "\t\t\temail: $data.jobApplication.email !== undefined ? $data.jobApplication.email : null,",
        "\t\t\tcv: $data.jobApplication.cv !== undefined ? $data.jobApplication.cv : null,",
        "\t\t\tcoverLetter: $data.jobApplication.coverLetter !== undefined ? $data.jobApplication.coverLetter : null",
        "\t\t},",
