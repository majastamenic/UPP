      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.jobApplication ){",
        "\treturn {",
        "\t\t//map jobApplication variable to expected task contract input",
        "\t\tjobApplicationInput: {",
        "\t\t\tname: $data.jobApplication.name !== undefined ? $data.jobApplication.name : null,",
        "\t\t\tsurname: $data.jobApplication.surname !== undefined ? $data.jobApplication.surname : null,",
        "\t\t\taddress: $data.jobApplication.address !== undefined ? $data.jobApplication.address : null,",
        "\t\t\temail: $data.jobApplication.email !== undefined ? $data.jobApplication.email : null,",
        "\t\t\tcv: $data.jobApplication.cv !== undefined ? $data.jobApplication.cv : null,",
        "\t\t\tcoverLetter: $data.jobApplication.coverLetter !== undefined ? $data.jobApplication.coverLetter : null",
        "\t\t},",
        "\t\tapprovedByHR: $data.formInput.approvedByHR",
        "\t}",
        "}"
      ],
