      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.conferenceRequest ){",
        "\treturn {",
        "\t\t//map conferenceRequest variable to expected task contract input",
        "\t\tconferenceRequestInput: {",
        "\t\t\tname: $data.conferenceRequest.name !== undefined ? $data.conferenceRequest.name : null,",
        "\t\t\tsurname: $data.conferenceRequest.surname !== undefined ? $data.conferenceRequest.surname : null,",
        "\t\t\temail: $data.conferenceRequest.email !== undefined ? $data.conferenceRequest.email : null,",
        "\t\t\tconferenceName: $data.conferenceRequest.conferenceName !== undefined ? $data.conferenceRequest.conferenceName : null,",
        "\t\t\tconferenceDate: $data.conferenceRequest.conferenceDate !== undefined ? $data.conferenceRequest.conferenceDate : null,",
        "\t\t\tprice: $data.conferenceRequest.price !== undefined ? $data.conferenceRequest.price : null,",
        "\t\t\tisOnline: $data.conferenceRequest.isOnline !== undefined ? $data.conferenceRequest.isOnline : null,",
        "\t\t\tapproved: $data.conferenceRequest.approved !== undefined ? $data.conferenceRequest.approved : null,",
        "\t\t\trejectDescription: $data.conferenceRequest.rejectDescription !== undefined ? $data.conferenceRequest.rejectDescription : null,",
        "\t\t\tallowedBudget: $data.conferenceRequest.allowedBudget !== undefined ? $data.conferenceRequest.allowedBudget : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
