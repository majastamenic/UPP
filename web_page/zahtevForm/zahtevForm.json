    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.zahtev ){",
        "\treturn {",
        "\t\t//map zahtev variable to expected task contract input",
        "\t\tzahtevInput: {",
        "\t\t\time: $data.zahtev.ime !== undefined ? $data.zahtev.ime : null,",
        "\t\t\tprezime: $data.zahtev.prezime !== undefined ? $data.zahtev.prezime : null,",
        "\t\t\tnazivDogadjaja: $data.zahtev.nazivDogadjaja !== undefined ? $data.zahtev.nazivDogadjaja : null,",
        "\t\t\tdatum: $data.zahtev.datum !== undefined ? $data.zahtev.datum : null,",
        "\t\t\tmesto: $data.zahtev.mesto !== undefined ? $data.zahtev.mesto : null,",
        "\t\t\tcena: $data.zahtev.cena !== undefined ? $data.zahtev.cena : null,",
        "\t\t\tjeOnline: $data.zahtev.jeOnline !== undefined ? $data.zahtev.jeOnline : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
